{
  "metadata": [
    {
      "src": [
        {
          "src": "source",
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ]
        }
      ],
      "dest": "docs/api/generated",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "net9.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "*.md",
          "toc.yml"
        ],
        "src": "docs"
      },
      {
        "files": [
          "**/*.md",
          "**/toc.yml"
        ],
        "src": "docs/api",
        "dest": "api"
      },
      {
        "files": [
          "**/*.yml"
        ],
        "src": "docs/api/generated",
        "dest": "api/generated"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "assets/**"
        ]
      },
      {
        "files": [
          "images/**",
          "assets/**"
        ],
        "src": "docs"
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "GamaLearn.Maui",
      "_appTitle": "GamaLearn.Maui Documentation",
      "_enableSearch": true,
      "_appFooter": "Â© GamaLearn - All rights reserved",
      "_appLogoPath": "images/Logo.png",
      "_gitContribute": {
        "repo": "https://github.com/GamaLearn/GamaLearn.Maui",
        "branch": "main"
      }
    },
    "markdownEngineProperties": {
      "markdigExtensions": [
        "advanced"
      ]
    }
  }
}