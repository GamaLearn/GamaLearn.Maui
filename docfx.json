{
  "metadata": [
    {
      "src": [
        {
          "src": "source",
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "net9.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "README.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "GamaLearn.Maui",
      "_appTitle": "GamaLearn.Maui Documentation",
      "_enableSearch": true
    },
    "markdownEngineProperties": {
      "markdigExtensions": [
        "advanced"
      ]
    }
  }
}